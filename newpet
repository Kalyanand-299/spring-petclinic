pipeline {
    agent {label 'JDK11 && MVN3 && NVM && PY3'} 
    stages {
        stage('clone') { 
            steps {
                git url:'https://github.com/Kalyanand-299/spring-petclinic.git' , branch : 'master' 
            }
        }
        stage('install') { 
            steps {
                sh 'mvn package'
            }
        }
    }
}